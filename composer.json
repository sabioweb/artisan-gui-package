{
    "name": "sabiowebcom/artisan-gui-package",
    "description": "Laravel package for executing Artisan commands through a web GUI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sabiowebcom Team",
            "email": "sabiowebcp@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0|^11.0",
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Sabiowebcom\\ArtisanGui\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sabiowebcom\\ArtisanGui\\Tests\\": "tests/",
            "Sabiowebcom\\ArtisanGui\\Database\\Factories\\": "database/factories/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sabiowebcom\\ArtisanGui\\ArtisanGuiServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

